{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "*":["./types/*.d.ts"]
    },
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ], 

  //   "rules": {
  //     "@typescript-eslint/no-unused-vars": "off",
  //     "no-unused-vars": "on",
  // }


}